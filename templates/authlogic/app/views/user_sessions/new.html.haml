%h1 Login
 
= form_for @user_session, :url => {:action => "create"} do |f|
  %fieldset 
    %ul
      %li
        = f.label :login
        = f.text_field :login
      %li
        = f.label :password
        = f.password_field :password
      %li
        = f.check_box :remember_me
        = f.label :remember_me
      %li
        = f.submit "Login"